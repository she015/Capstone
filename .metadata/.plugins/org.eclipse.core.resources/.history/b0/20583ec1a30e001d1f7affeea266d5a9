jwt.token.validity=18000
jwt.signing.key=signingkey
jwt.authorities.key=roles
jwt.token.prefix=Bearer
jwt.header.string=Authorization

# MSQL DATABASE CONFIG
spring.datasource.url=jdbc:mysql://localhost:3306/cts
spring.datasource.username=root
spring.datasource.password=root
spring.jpa.hibernate.ddl-auto=update
spring.user.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect

spring.main.allow-circular-references=true
server.port=8086

# SWAGGER
spring.mvc.pathmatch.matching-strategy=ant-path-matcher

# Eureka
 spring.application.name=security-role-jwt-microservice
 eureka.client.serviceUrl.defaultZone  = http://localhost:8085/eureka
 eureka.client.registerWithEureka=true
eureka.client.fetchRegistry=true



